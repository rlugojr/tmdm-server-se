	<!-- Update report type -->
	<xsd:element abstract="false" name="Update" nillable="false"
		type="Update">
		<xsd:unique name="Update">
			<xsd:selector xpath="." />
			<xsd:field xpath="Source" />
			<xsd:field xpath="TimeInMillis" />
