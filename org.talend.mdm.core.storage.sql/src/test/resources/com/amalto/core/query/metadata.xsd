<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:import namespace="http://www.w3.org/2001/XMLSchema" />
	<xsd:simpleType name="MULTI_LINGUAL">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\[\w+\:[^\[\]]*\]){0,}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="limitedInt_10">
		<xsd:restriction base="xsd:integer">
			<xsd:maxInclusive value="10" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="limitedString_15">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="15" />
		</xsd:restriction>
	</xsd:simpleType>

	<!-- PERSON DATA MODEL -->

	<xsd:element name="Person">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="id" type="xsd:string" />
				<xsd:element name="firstname" type="xsd:string" />
				<xsd:element name="middlename" type="xsd:string" />
				<xsd:element name="lastname" type="xsd:string" />
				<xsd:element maxOccurs="1" minOccurs="0" name="resume"
					type="MULTI_LINGUAL" />
				<xsd:element name="age" type="xsd:positiveInteger" />
				<xsd:element name="score" type="xsd:decimal" />
				<xsd:element minOccurs="0" name="Available" type="xsd:boolean" />
				<xsd:element maxOccurs="unbounded" minOccurs="1" name="addresses">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element maxOccurs="unbounded" name="address"
								type="xsd:string">
								<xsd:annotation>
									<xsd:appinfo source="X_ForeignKey">Address</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="0" name="knownAddresses">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element maxOccurs="unbounded" name="knownAddress"
								type="Address" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="1" name="Status"
					type="PersonStatus">
					<xsd:annotation>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Hide">System_Users</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
		<xsd:unique name="Person">
			<xsd:selector xpath="." />
			<xsd:field xpath="id" />
		</xsd:unique>
	</xsd:element>

	<xsd:complexType name="Address">
		<xsd:sequence>
			<xsd:element name="Street" type="xsd:string" />
			<xsd:element name="City" type="xsd:string" />
			<xsd:element name="Phone" type="limitedInt_10" />
			<xsd:element name="Notes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Note" minOccurs="0" maxOccurs="unbounded"
							type="xsd:string" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="Customer">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="id" type="xsd:string" />
				<xsd:element name="firstname" type="xsd:string" />
				<xsd:element name="middlename" type="xsd:string" />
				<xsd:element name="lastname" type="xsd:string" />
				<xsd:element minOccurs="0" name="Available" type="xsd:boolean" />
				<xsd:element maxOccurs="1" name="address1" type="Address" />
				<xsd:element maxOccurs="1" name="address2" type="Address" />
			</xsd:sequence>
		</xsd:complexType>
		<xsd:unique name="Customer">
			<xsd:selector xpath="." />
			<xsd:field xpath="id" />
		</xsd:unique>
	</xsd:element>


	<xsd:simpleType name="PersonStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Customer" />
			<xsd:enumeration value="Friend" />
			<xsd:enumeration value="Employee" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="Address">
		<xsd:annotation>
			<xsd:appinfo source="X_Label_FR">Adresse</xsd:appinfo>
			<xsd:appinfo source="X_Label_EN">Address</xsd:appinfo>
			<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
			<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="1" minOccurs="1" name="id"
					type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo source="X_Label_FR">id (s?quence)</xsd:appinfo>
						<xsd:appinfo source="X_Label_EN">id (sequence)</xsd:appinfo>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="1" name="Street"
					type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo source="X_Label_FR">Rue</xsd:appinfo>
						<xsd:appinfo source="X_Label_EN">Street</xsd:appinfo>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="1" name="ZipCode"
					type="xsd:integer">
					<xsd:annotation>
						<xsd:appinfo source="X_Label_FR">Code postal</xsd:appinfo>
						<xsd:appinfo source="X_Label_EN">Zip code</xsd:appinfo>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="1" name="enterprise"
					type="xsd:boolean">
					<xsd:annotation>
						<xsd:appinfo source="X_Label_FR">Code postal</xsd:appinfo>
						<xsd:appinfo source="X_Label_EN">Zip code</xsd:appinfo>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="1" name="City"
					type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo source="X_Label_FR">Ville</xsd:appinfo>
						<xsd:appinfo source="X_Label_EN">City</xsd:appinfo>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="0" name="OptionalCity"
					type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo source="X_Label_FR">Ville optionelle</xsd:appinfo>
						<xsd:appinfo source="X_Label_EN">Optional city</xsd:appinfo>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="unbounded" minOccurs="0" name="Remark"
					type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo source="X_Create">System_Admin</xsd:appinfo>
						<xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="1" name="country"
					type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo source="X_ForeignKey">Country/id</xsd:appinfo>
						<xsd:appinfo source="X_ForeignKeyInfo">Country/name</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element maxOccurs="1" minOccurs="0" name="score"
					type="xsd:decimal" />
			</xsd:sequence>
		</xsd:complexType>
		<xsd:unique name="Address">
			<xsd:selector xpath="." />
			<xsd:field xpath="id" />
			<xsd:field xpath="enterprise" />
		</xsd:unique>
	</xsd:element>

	<xsd:element name="Country">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="id" type="xsd:integer" />
				<xsd:element name="creationDate" type="xsd:date" />
				<xsd:element name="creationTime" type="xsd:dateTime" />
				<xsd:element name="name" type="xsd:string" />
				<xsd:element maxOccurs="unbounded" minOccurs="0" name="notes">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element minOccurs="1" maxOccurs="1" name="note"
								type="xsd:string" />
							<xsd:element minOccurs="0" maxOccurs="unbounded"
								name="comment" type="xsd:string" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
		<xsd:unique name="Country">
			<xsd:selector xpath="." />
			<xsd:field xpath="id" />
		</xsd:unique>
	</xsd:element>
	
	<xsd:element name="CountryLong">
